  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack serve --mode development --open",
    "build:dev": "webpack --mode development",
    "watch": "webpack --watch --mode development",
    "analyze": "webpack --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run test",
    "test:e2e": "cross-env NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --config jest.e2e.config.cjs --silent",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "format": "prettier --write 'src/**/*.{js,jsx,css,json}'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "husky install",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "backsty",
  "license": "ISC",
