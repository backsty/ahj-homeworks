{
  "name": "goblin-game",
  "version": "1.0.0",
  "description": "Goblin Game",
  "repository": {
    "type": "git",
    "url": "https://github.com/backsty/ahj-homeworks/tree/video/events"
  },
  "main": "./src/index.js",
  "private": true,
  "scripts": {
    "start": "yarn webpack serve --config webpack.dev.js",
    "build": "yarn webpack --config webpack.prod.js",
    "watch": "yarn webpack --watch --config webpack.dev.js",
    "dev": "yarn clean && yarn build && yarn start",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch", 
    "test:coverage": "yarn jest --coverage",
    "preshow:coverage": "yarn coverage",
    "show:coverage": "yarn live-server coverage/lcov-report",
    "preshow:dist": "yarn build",
    "show:dist": "yarn http-server dist",
    "format": "yarn prettier --write \"src/**/*.{js,css,json}\"",
    "format:check": "yarn prettier --check \"src/**/*.{js,css,json}\"",
    "lint": "yarn eslint .",
    "lint:fix": "yarn eslint . --fix",
    "lint:styles": "yarn stylelint \"src/**/*.css\"",
    "clean": "yarn rimraf dist",
    "prebuild": "yarn clean"
  },
  "keywords": [],
  "author": "backsty",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/runtime": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cypress": "^13.16.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "globals": "^15.13.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "core-js": "^3.39.0"
  }
}
